{
  "expo": {
    "scheme": "wuzz",
    "name": "wuzz",
    "slug": "wuzz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-wuzz.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,

    "splash": {
      "backgroundColor": "#000000"
    },

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-wuzz.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true
    },

    "web": {
      "favicon": "./assets/favicon.png"
    },

    "plugins": ["expo-router"]
  }
}
